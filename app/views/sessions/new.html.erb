<h1>Sign In</h1>

<%= form_with(url: session_path) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
    </div>

    <div>
      <%= form.submit "Sign In" %>
    </div>
<% end %>
